{
    "name": "1688ImagesCrawling",
    "description": "Get 1688 Images :D",
    "version": "1.0",
	"background": {
      "scripts": ["scripts/jquery-3.5.1.min.js", "background.js"],
	  "persistent": true
    },
	"content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "scripts/jquery-3.5.1.min.js", "content.js"
        ],
        "run_at": "document_start"
        }
    ], 
    "permissions":[
		"tabs",
        "activeTab",
		"storage",
		"contextMenus"
    ],
    "browser_action":{
        "default_title": "Get 1688 Images",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
	"icons": {
	   "16": "icon16.png",
	  "128": "icon128.png"
	}
}